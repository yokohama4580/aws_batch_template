data "aws_iam_role" "ecsInstanceRole" {
  name  = "ecsInstanceRole"
}

data "aws_iam_role" "awsBatchServiceRole" {
  name  = "AWSBatchServiceRole"
}

resource "aws_iam_instance_profile" "prd_ecs_instance_role" {
  name  = "batch_execute_platform_role"
  role  = "${data.aws_iam_role.ecsInstanceRole.name}"
}